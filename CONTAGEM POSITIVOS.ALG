Algoritmo "Contagem positivos"
Var
c,cp,n:inteiro
Inicio
      c <- 1
      cp <- 0
      enquanto c <= 10 faca
      escreval("Digite um numero:")
      leia(n)
      escreval ("Numero digitado:",n)
      c <- c +1
      se  n > 0 entao
      cp <- cp + 1
      senao
      fimse
      fimenquanto
      escreva("Numeros positivos:",cp)

Fimalgoritmo

