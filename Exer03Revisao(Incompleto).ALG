Algoritmo "semnome"

Var

vlringresso,AuAudiencia,AuInicial,Aufinal:inteiro
Inicio

      AuInicial <- 200
      AuAudiencia <- 52
      para vlringresso de 10 ate 1 passo -1 faca
        escreval("Valor ingresso: ",vlringresso)
        AuAudiencia <- AuAudiencia + 52
        escreval("Publico atingido: ",AuAudiencia)
      fimpara
      AuFinal <- AuInical + Audiencia

Fimalgoritmo