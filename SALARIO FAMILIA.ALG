Algoritmo "Salario familia"

Var
c,sf,sm,mf:real


Inicio
      c <- 1
      sf <-0
      escreva("Numero de membros da familia:")
      leia(mf)
      enquanto (c <= mf) faca
      escreva("Salario membro:")
      leia(sm)
      sf <- sf + sm
      c <- c +1
      fimenquanto
      escreval("Salario da familia é:",sf)

Fimalgoritmo